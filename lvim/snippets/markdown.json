{
  "tt": {
    "prefix": "tt",
    "body": [
      "---",
      "tags: [project, $1]",
      "---",
      "",
      "= ${TM_FILENAME} =",
      "== $2 | status:pending +${1|office,home,computer|} project:$2 =",
      "* $3"
    ],
    "description": "Project tag"
  },
  "to": {
    "prefix": "to",
    "body": [
      "---",
      "tags: [project, office]",
      "---",
      "",
      "= ${TM_FILENAME} =",
      "== $2 | status:pending +office project:$2 =",
      "* $3"
    ],
    "description": "Office project tag"
  },
  "th": {
    "prefix": "th",
    "body": [
      "---",
      "tags: [project, home]",
      "---",
      "",
      "= ${TM_FILENAME} =",
      "== $2 | status:pending +home project:$2 ==",
      "* $3"
    ],
    "description": "Home project tag"
  },
  "tag": {
    "prefix": "tag",
    "body": [
      "---",
      "tags: [project]",
      "---",
      "",
      "= ${TM_FILENAME}",
      "$2"
    ],
    "description": "Project tag"
  },
  "tte": {
    "prefix": "tte",
    "body": [
      "---",
      "tags: [evergreen, $1]",
      "---",
      "",
      "= ${TM_FILENAME}",
      "$2"
    ],
    "description": "Evergreen tag"
  },
  "tw": {
    "prefix": "tw",
    "body": [
      "= __waiting_for || status:pending +waiting_for =",
      "== $2 | project:$2 ==",
      "* $3"
    ],
    "description": "Waiting for tag"
  },
  "tp": {
    "prefix": "tp",
    "body": [
      "== $1 | project:$1"
    ],
    "description": "Create project"
  }
}
