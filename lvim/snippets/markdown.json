    "body": [
      "---",
      "tags: [project, office]",
      "---",
      "= `!p snip.rv = snip.basename` =",
      "== $2 | status:pending +office project:$2 ==",
      "* $3"
    ],
    "description": "Office project tag"
  },
  "th": {
    "prefix": "th",
    "body": [
      "---",
      "tags: [project, home]",
      "---",
      "= `!p snip.rv = snip.basename` =",
      "== $2 | status:pending +home project:$2 ==",
      "* $3"
    ],
    "description": "Home project tag"
  },
  "tag": {
    "prefix": "tag",
    "body": [
      "---",
      "tags: [project]",
      "---",
      "= `!p snip.rv = snip.basename` ="
