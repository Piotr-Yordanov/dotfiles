{
  "tt": {
    "prefix": "tt",
    "body": [
      "---",
      "tags: [project, $1]",
      "---",
      "= `!p snip.rv = snip.basename` =",
      "== $2 | status:pending +${1|office,home,computer|} project:$2 ==",
      "* $3"
    ],
    "description": "Project tag"
  },
  "to": {
